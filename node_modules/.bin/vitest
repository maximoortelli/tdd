#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/user/Desktop/miduactions/tdd/node_modules/.pnpm/vitest@1.3.1_happy-dom@13.6.2/node_modules/vitest/node_modules:/mnt/c/Users/user/Desktop/miduactions/tdd/node_modules/.pnpm/vitest@1.3.1_happy-dom@13.6.2/node_modules:/mnt/c/Users/user/Desktop/miduactions/tdd/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/user/Desktop/miduactions/tdd/node_modules/.pnpm/vitest@1.3.1_happy-dom@13.6.2/node_modules/vitest/node_modules:/mnt/c/Users/user/Desktop/miduactions/tdd/node_modules/.pnpm/vitest@1.3.1_happy-dom@13.6.2/node_modules:/mnt/c/Users/user/Desktop/miduactions/tdd/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vitest@1.3.1_happy-dom@13.6.2/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/vitest@1.3.1_happy-dom@13.6.2/node_modules/vitest/vitest.mjs" "$@"
fi
